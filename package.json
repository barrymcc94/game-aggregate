        "test:watch": "jest --watch",
        "test:updateSnapshot": "jest --updateSnapshot",
        "lint": "eslint --ext .js --ext .jsx src",
        "format": "prettier --config .prettierrc '{,!(node_modules|build|coverage)/**/}*.(js|jsx|json)' --write"
    },
    "author": "Barry Mc Cormack",
