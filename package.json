    "scripts": {
        "start": "webpack-dev-server --hot --host 0.0.0.0 --config webpack.config.dev.js --mode development",
        "build": "rm -rf ./build && webpack --config webpack.config.prod.js --progress --colors --mode production && cp -r ./public/. ./build",
        "start-build": "http-server build",
        "json-server": "json-server --watch ./api/index.js --port 3001 --delay 500",
